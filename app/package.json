{
  "name": "kuwau-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3030",
  "scripts": {
    "assets:dev": "cp -R public/* dev/",
    "assets:build": "cp -R public/* build/",
    "html": "pug src/html -o dev -w",
    "prestart": "rm -rf dev && mkdir dev",
    "start": "npm-run-all --parallel html assets:dev bundle",
    "bundle": "webpack-dev-server",
    "prebuild": "rm -rf build && mkdir build",
    "build": "webpack",
    "staging": "rm -rf ../server/build && mv build ../server/build",
    "build:staging": "npm-run-all --serial build staging",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "date-fns": "^2.0.0-alpha.1",
    "moment": "^2.18.1",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-addons-shallow-compare": "^15.5.2",
    "react-avatar-editor": "^9.1.1",
    "react-color": "^2.12.0",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-share": "^1.13.2",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-observable": "^0.14.1",
    "redux-persist": "^4.6.0",
    "rxjs": "^5.2.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.3",
    "socket.io-client": "^2.0.1",
    "superagent": "^3.5.2",
    "webpack-dev-server": "^2.5.1"
  },
  "devDependencies": {
    "anser": "^1.2.7",
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "^7.0.0-beta.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "^0.28.0",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-loader": "^1.3.3",
    "promise": "7.1.1",
    "pug-cli": "^1.0.0-alpha6",
    "react-dev-utils": "^0.5.2",
    "stack-frame-resolver": "^0.1.5",
    "style-loader": "^0.16.1",
    "sw-precache": "^5.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^@components(.*)$": "<rootDir>/src/js/components$1",
      "^@elements(.*)$": "<rootDir>/src/js/elements$1",
      "^@pages(.*)$": "<rootDir>/src/js/pages$1",
      "^@actions(.*)$": "<rootDir>/src/js/redux/actions$1",
      "^@utils(.*)$": "<rootDir>/src/js/utils$1"
    }
  }
}
